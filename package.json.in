{
    "name": "frcl",
    "version": "@FRCL_VERSION@",
    "type": "module",
    "main": "./dist/core.cjs.js",
    "module": "./dist/core.es.js",
    "exports" : {
        ".": {
            "import": "./dist/core.es.js",
            "require": "./dist/core.cjs.js"
        },
        "./component": {
            "import": "./dist/component.es.js",
            "requere": "./dist/component.cjs.js"
        },
        "./frcl.css": "./dist/assets/frcl.css"
    },
    "author": "s3mat3",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/s3mat3/frcl"
    },
    "scripts": {
        "dev": "vite --mode development",
        "build": "vite build --mode production",
        "dbuild": "vite build --mode development",
        "preview": "vite preview",
        "test": "vitest run --printConsoleTrace=true",
        "dtest": "vitest watch --printConsoleTrace=true",
        "docs": "jsdoc --private --configure ./jsdoc.json --verbose"
    },
    "devDependencies": {
        "docdash": "^2.0.2",
        "jsdoc": "^4.0.4",
        "jsdom": "^26.0.0",
        "json-server": "^1.0.0-beta.3",
        "material-symbols": "^0.29.2",
        "sass": "^1.86.3",
        "vite": "^6.2.0",
        "vitest": "^3.0.7"
    }
}
